{
  "executable": "StartingPoint",
  "splashscreen": {
    "imageFile": ""
  },
  "name": "StartingPoint",
  "author": "",
  "singleInstance": false,
  "environment": "Production",
  "aspCoreBackendPort": 8085,
  "build": {
    "appId": "com.StartingPoint.app",
    "productName": "StartingPoint",
    "copyright": "Copyright Â© 2024",
    "buildVersion": "1.0.0",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "mac": {
      "icon": "../../../wwwroot/icon.png"
    },
    "win": {
      "icon": "../../../wwwroot/icon.png"
    },
    "linux": {
      "icon": "../../../wwwroot/icon.png"
    },
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  }
}